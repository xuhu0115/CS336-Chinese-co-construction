    "#  1. è‡ªå®šä¹‰çº¿æ€§å±‚çš„å®ç°ï¼ˆæ²¡æœ‰åç½®é¡¹ï¼‰\n",
    "import torch\n",
    "import torch.nn as nn\n",
    "import torch.nn.init as init\n",
    "import math\n",
    "\n",
    "class Linear(nn.Module):\n",
    "    def __init__(self, in_features, out_features, device=None, dtype=None):\n",
