    "import torch\n",
    "import os\n",
    "import typing\n",
    "\n",
    "def run_save_checkpoint(model: torch.nn.Module, optimizer: torch.optim.Optimizer, iteration: int, out: typing.Union[str, os.PathLike, typing.BinaryIO]):\n",
